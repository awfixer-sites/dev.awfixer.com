{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "AW&Friends Developer Docs",
  "colors": {
    "primary": "#050430",
    "light": "#51EFFE",
    "dark": "#050430"
  },
  "favicon": "/favicon.jpeg",
  "navigation": {
    "tabs": [
      {
        "tab": "Guides",
        "groups": [
          {
            "group": "Getting started",
            "pages": ["index", "quickstart", "development"]
          }
        ]
      },
      {
        "tab": "API reference",
        "groups": [
          {
            "group": "API documentation",
            "pages": ["api-reference/introduction"]
          },
          {
            "group": "Endpoint examples",
            "pages": [
              "api-reference/endpoint/get",
              "api-reference/endpoint/create",
              "api-reference/endpoint/delete",
              "api-reference/endpoint/webhook"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Discord",
          "href": "https://discord.awfixer.com",
          "icon": "discord"
        },
        {
          "anchor": "Patreon",
          "href": "https://patreon.com/awfixer",
          "icon": "newspaper"
        },
        {
          "anchor": "Help Center",
          "href": "https://help.awfixer.com",
          "icon": "book-open-cover"
        }
      ]
    }
  },
  "logo": {
    "light": "/favicon.jpeg",
    "dark": "/favicon.jpeg"
  },
  "navbar": {
    "links": [
      {
        "label": "Help Center",
        "href": "https://help.awfixer.com"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Community",
      "href": "https://discord.awfixer.com"
    }
  },
  "contextual": {
    "options": [
      "copy",
      "view",
      "chatgpt",
      "claude",
      "perplexity",
      "mcp",
      "cursor",
      "vscode"
    ]
  },
  "footer": {
    "socials": {
      "x": "https://x.com/awfixer",
      "github": "https://github.com/awfixers-stuff",
      "discord": "https://discord.awfixer.com"
    }
  }
}
